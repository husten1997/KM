<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE themes PUBLIC "-//www.matthiasmann.de//TWL-Theme//EN" "http://hg.l33tlabs.org/twl/raw-file/tip/src/de/matthiasmann/twl/theme/theme.dtd">
<themes>
    <images file="simple.png" format="RGBA">
		<area name="-frameA" xywh="0,0,9,9" splitx="L3,R3" splity="T3,B3"/>
		<area name="-frameB" xywh="10,0,9,9" splitx="L3,R3" splity="T3,B3"/>
		<area name="-frameC" xywh="20,0,9,9" splitx="L3,R3" splity="T3,B3"/>
		<area name="-frameD" xywh="30,0,9,9" splitx="L3,R3" splity="T3,B3"/>
		<area name="-frameE" xywh="40,0,9,9" splitx="L3,R3" splity="T3,B3"/>
		<area name="-frameF" xywh="50,0,9,9" splitx="L3,R3" splity="T3,B3"/>
		<area name="-frameG" xywh="60,0,9,9" splitx="L3,R3" splity="T3,B3"/>
		<area name="-frameH" xywh="70,0,9,9" splitx="L3,R3" splity="T3,B3"/>
		<select name="button.background">
			<alias ref="-frameB" if="disabled" tint="#8fff"/>
			<alias ref="-frameE" if="armed"/>
			<alias ref="-frameB"/>
		</select>
	</images>
	<images file="a.png" format="RGBA">
		<area name="-frameAA" xywh="0,0,32,32" splitx="L3,R3" splity="T3,B3"/>
		<area name="-frameBB" xywh="0,0,32,32" splitx="L3,R3" splity="T3,B3"/>
		<select name="test">
			<alias ref="-frameAA" if="disabled" tint="#8fff"/>
			<alias ref="-frameBB" if="armed"/>
			<alias ref="-frameAA"/>
		</select>
	</images>
	
	<images file="mm.png" format="RGBA">
		<area name="-frameMenue" xywh="0,0,3648,688" splitx="L3,R3" splity="T3,B3"/>
		<select name="baumenue">
			<alias ref="-frameMenue" if="disabled" tint="#8fff"/>
			<alias ref="-frameMenue" if="armed"/>
			<alias ref="-frameMenue"/>
		</select>
	</images>
	
	<images file="url.png" format="RGBA">
		<area name="-frameM" xywh="0,0,1920,362" splitx="L3,R3" splity="T3,B3"/>
		<select name="bauM">
			<alias ref="-frameM" if="disabled" tint="#8fff"/>
			<alias ref="-frameM" if="armed"/>
			<alias ref="-frameM"/>
		</select>
	</images>
	
    <fontDef name="normal" filename="font.fnt" color="white" default="true">
        <fontParam if="error" color="red"/>
        <fontParam if="warning" color="orange"/>
        <fontParam if="disabled" color="gray"/>
        <fontParam if="textSelection" color="blue"/>
    </fontDef>
	
	
	
    <inputMapDef name="-defaultInputMap">
        <action name="selectAll">ctrl A</action>
        <action name="selectAll">cmd A</action>
        <action name="cut">ctrl X</action>
        <action name="cut">cmd X</action>
        <action name="copy">ctrl C</action>
        <action name="copy">cmd C</action>
        <action name="paste">ctrl V</action>
        <action name="paste">cmd V</action>
    </inputMapDef>

    <theme name="-defaults">
        <param name="background"><image>none</image></param>
        <param name="overlay"><image>none</image></param>
        <param name="font"><font>normal</font></param>
        <param name="textAlignment"><enum type="alignment">center</enum></param>
        <param name="minWidth"><int>0</int></param>
        <param name="minHeight"><int>0</int></param>
        <param name="maxWidth"><int>0</int></param>
        <param name="maxHeight"><int>0</int></param>
        <param name="inputMap"><inputMap>-defaultInputMap</inputMap></param>
    </theme>
    
    <theme name="tooltipwindow" ref="-defaults">
        <param name="fadeInTime"><int>0</int></param>
    </theme>
    
    <theme name="button" ref="-defaults">
		<param name="background"><image>button.background</image></param>
        <param name="minHeight"><int>10</int></param>
        <param name="minWidth"><int>10</int></param>
	</theme>
	
	<theme name="main" ref="-defaults">
		<theme name="button_Test" ref="button">
			<param name=""><image>button.*</image></param>
			<param name="minHeight"><int>10</int></param>
			<param name="minWidth"><int>10</int></param>
		</theme>
	</theme>
	
	
	
	<theme name="label" ref="-defaults" allowWildcard="true">
    </theme>
	
    <theme name="resizableframe" ref="-defaults">
        <param name="background"><image>bauM</image></param>
		<param name="offscreenExtra"><border>13,15</border></param>
        <param name="titleAreaTop"><int>5</int></param>
        <param name="titleAreaLeft"><int>5</int></param>
        <param name="titleAreaRight"><int>-5</int></param>
        <param name="titleAreaBottom"><int>5</int></param>
        <param name="hasCloseButton"><bool>false</bool></param>
        <param name="closeButtonX"><int>0</int></param>
        <param name="closeButtonY"><int>0</int></param>
        <param name="hasResizeHandle"><bool>false</bool></param>
        <param name="resizeHandleX"><int>0</int></param>
        <param name="resizeHandleY"><int>0</int></param>
        <param name="mouseCursor."><cursor>eforen.cursor.arrow.*</cursor></param>
        <param name="fadeColorInactive"><color>#FFFF</color></param>
        <param name="fadeDurationActivate"><int>200</int></param>
        <param name="fadeDurationDeactivate"><int>200</int></param>
        <param name="fadeDurationShow"><int>200</int></param>
        <param name="fadeDurationHide"><int>200</int></param>
        <theme name="title" ref="label">
            <param name="textAlignment"><enum type="alignment">top</enum></param>
        </theme>
        <theme name="closeButton" ref="button">
            <param name=""><image>eforen.frame.closebutton.*</image></param>
            <param name="border"><border>background.border</border></param>
            <param name="minWidth"><int>0</int></param>
        </theme>
        <theme name="button_Test" ref="button">
			<param name=""><image>button.*</image></param>
			<param name="minHeight"><int>10</int></param>
			<param name="minWidth"><int>10</int></param>
		</theme>
        <theme name="resizeHandle" ref="-defaults"/>
    </theme>
    
    <theme name="knightmare" ref="-defaults">
		<theme name="button_Test" ref="button">
			<param name=""><image>button.*</image></param>
			<param name="minHeight"><int>10</int></param>
			<param name="minWidth"><int>10</int></param>
		</theme>
		
		<theme name="label" ref="-defaults" allowWildcard="true">
		
   		</theme>
   		
   		<theme name="frame" ref="resizableframe">
   		</theme>
	</theme>
	
	

    
</themes>

	<theme name="-defaults">
		<param name="cursor">
			<cursor>cursor1</cursor>
		</param>
		<param name="background">
			<image>none</image>
		</param>
		<param name="overlay">
			<image>none</image>
		</param>
		<param name="font">
			<font>normal</font>
		</param>
		<param name="textAlignment">
			<enum type="alignment">center</enum>
		</param>
		<param name="minWidth">
			<int>0</int>
		</param>
		<param name="minHeight">
			<int>0</int>
		</param>
		<param name="maxWidth">
			<int>0</int>
		</param>
		<param name="maxHeight">
			<int>0</int>
		</param>
		<param name="inputMap">
			<inputMap>-defaultInputMap</inputMap>
		</param>
	</theme>
	<theme name="tooltipwindow" ref="-defaults">
		<param name="background">
			<image>-frameH</image>
		</param>
		<param name="border">
